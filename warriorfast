    local function Teleport(world)
    	local args = {
        	[1] = world
    	}
    	ReplicatedStorage.Remotes.Common.Door.Teleport:FireServer(unpack(args))
	end

    -- Toggles
    local Tab = Window:MakeTab({
        Name = "Farm",
        Icon = "rbxassetid://15403180883",
        PremiumOnly = false
    })

